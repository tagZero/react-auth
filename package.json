{
  "name": "@tag0/react-auth",
  "version": "2.0.1",
  "description": "Customizable AuthService/AuthProvider and components for react",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tagZero/react-auth.git"
  },
  "keywords": [
    "auth",
    "authn",
    "authentication",
    "react",
    "auth-components",
    "react-components",
    "auth-library",
    "react-auth"
  ],
  "types": "dist/react-auth.d.ts",
  "main": "dist/cjs/react-auth.js",
  "module": "dist/es/react-auth.js",
  "browser": "dist/umd/react-auth.js",
  "license": "MIT",
  "files": [
    "dist",
    "package.json",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest --config=jest.config.js",
    "build": "yarn clean && NODE_ENV=development rollup -c rollup.config.js",
    "build:prod": "yarn build && NODE_ENV=production rollup -c rollup.config.js",
    "build:example": "yarn clean && rollup -c rollup.examples.config.js",
    "start": "yarn build:example && serve -s dist",
    "prepublishOnly": "yarn build:prod",
    "watch": "npm-watch"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@wessberg/rollup-plugin-ts": "^1.3.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "npm-watch": "^0.7.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^17.0.1",
    "rollup": "^2.38.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-filesize": "^9.1.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^11.3.2",
    "ts-jest": "^26.4.4",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": ">=16.x",
    "react-dom": ">=16.x",
    "react-router-dom": ">=5.x"
  },
  "dependencies": {
    "react-toastify": "^7.0.3"
  },
  "watch": {
    "start": {
      "extensions": "ts,tsx,js,jsx,json,css,sass,scss",
      "patterns": [
        "src"
      ]
    }
  }
}
